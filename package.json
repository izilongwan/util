{
  "name": "@izilong/util",
  "version": "1.0.59",
  "description": "@izilong/util",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "node": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "lib"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "babel": {
    "presets": [
      "env",
      "es2015"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false
        }
      ]
    ]
  },
  "scripts": {
    "esm-cjs": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "build:umd": "mode=umd npx webpack",
    "build:esm": "mode=esm npx webpack",
    "build": "npm run build:umd && npm run build:esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izilongwan/util.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/izilongwan/util/issues"
  },
  "homepage": "https://github.com/izilongwan/util#readme"
}
